    <selectionEntry type="upgrade" import="true" name="Spectre Jet Pack" hidden="false" id="edd3-50cf-9c45-aca0" publicationId="8666-c70e-13e4-5f7a" page="164" collective="false">
      <costs>
        <cost name="Pts" typeId="d2ee-04cb-5f8a-2642" value="0"/>
